   "source": [
    "# Simple Factorial using WHILE loop\n",
    "\n",
    "def factorial(number):\n",
    "    start = 1\n",
    "    output = start\n",
    "    i = 1\n",
    "    while i < number:\n",
    "        output = output*(start+i)\n",
    "        i += 1\n",
    "    return(output)"
   ]
  }
